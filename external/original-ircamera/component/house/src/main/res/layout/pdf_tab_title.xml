<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   android:paddingStart="13dp"
                                                   android:paddingEnd="13dp"
                                                   tools:ignore="SmallSp">

    <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#f4f4f4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <View
            android:id="@+id/view_title1"
            android:layout_width="0dp"
            android:layout_height="1dp"
            app:layout_constraintHorizontal_weight="65"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv_title2"/>

    <TextView
            android:id="@+id/tv_title2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="@string/house_state_good"
            android:textSize="6sp"
            android:textColor="#666666"
            app:layout_constraintHorizontal_weight="45"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/view_title1"
            app:layout_constraintEnd_toStartOf="@+id/tv_title3"/>

    <TextView
            android:id="@+id/tv_title3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="@string/house_state_repair"
            android:textSize="6sp"
            android:textColor="#666666"
            app:layout_constraintHorizontal_weight="45"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tv_title2"
            app:layout_constraintEnd_toStartOf="@+id/tv_title4"/>

    <TextView
            android:id="@+id/tv_title4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="@string/house_state_replace"
            android:textSize="6sp"
            android:textColor="#666666"
            app:layout_constraintHorizontal_weight="45"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tv_title3"
            app:layout_constraintEnd_toStartOf="@+id/tv_title5"/>

    <TextView
            android:id="@+id/tv_title5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:paddingTop="4dp"
            android:paddingBottom="4dp"
            android:text="@string/report_remark"
            android:textSize="6sp"
            android:textColor="#666666"
            app:layout_constraintHorizontal_weight="158"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/tv_title4"
            app:layout_constraintEnd_toEndOf="parent"/>

    <View
            android:layout_width="0dp"
            android:layout_height="0.5dp"
            android:background="#dddddd"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <View
            android:layout_width="0dp"
            android:layout_height="0.5dp"
            android:background="#dddddd"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintEnd_toEndOf="@id/view_title1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintEnd_toEndOf="@id/tv_title2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintEnd_toEndOf="@id/tv_title3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintEnd_toEndOf="@id/tv_title4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <View
            android:layout_width="0.5dp"
            android:layout_height="0dp"
            android:background="#dddddd"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size
                    android:width="24dp"
                    android:height="24dp" />
            <solid android:color="#ECF5FF" />
        </shape>
    </item>

    <item
            android:bottom="6dp"
            android:end="6dp"
            android:start="6dp"
